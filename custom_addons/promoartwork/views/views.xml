<odoo>
  <data>
    <!-- explicit list view definition -->
    
    <record model="ir.ui.view" id="orders_tree">
      <field name="name">Artwork Requests</field>
      <field name="model">virtual.artwork</field>
      <field name="arch" type="xml">
        <tree create="false">
          <field name="name"/>
          <field name="customer_name"/>
          <field name="instructions"/>
          <field name="artwork_file"/>
          <!-- <field name="color_id"/> -->
          <field name="process_id"/>
          <!-- <field name="jobType_id"/> -->
          <field name="outputformat_id"/>
          <field name="turnaround_id"/>
          <field name="service_type"/>
          <field name="employee"/>
        </tree>
      </field>
    </record>
    <record model="ir.ui.view" id="orders_form">
      <field name="name">Artwork Requests</field>
      <field name="model">virtual.artwork</field>
      <field name="arch" type="xml">
        <form create="false">
          <group>
            <group>
              <field name="name"/>
              <field name="artwork_file"/>
              <field name="service_type"/>
              <field name="instructions"/>
            </group>
            <group>
              <field name="process_id"/>
              <field name="outputformat_id"/>
              <field name="turnaround_id"/>
            </group>
          </group>
          <field name="name"/>
          <field name="customer_name"/>
          <field name="instructions"/>
          <field name="artwork_file"/>
          <!-- <field name="color_id"/> -->
          <field name="process_id"/>
          <field name="jobType_id"/>
          <field name="outputformat_id"/>
          <field name="turnaround_id"/>
          <field name="service_type"/>
          <field name="employee"/>
        </form>
      </field>
    </record>

    <record model="ir.ui.view" id="request_quotes">
      <field name="name">Request Quotes</field>
      <field name="model">request.quote</field>
      <field name="arch" type="xml">
        <tree>
          <field name="customer_name"/>
          <field name="instructions"/>
          <field name="artwork_file"/>
          <field name="process_id"/>
          <field name="jobType_id"/>
          <field name="outputformat_id"/>
          <field name="turnaround_id"/>
          <field name="expected_charges"/>
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="job_types_tree">
      <field name="name">Job Types</field>
      <field name="model">job.types</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="name"/>
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="size_properties_tree">
      <field name="name">Sizes For Website</field>
      <field name="model">size.properties</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="name"/>
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="imprint_colors_tree">
      <field name="name">Imprint Colors</field>
      <field name="model">imprint.colors</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="name"/>
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="imprint_process_tree">
      <field name="name">Imprint Process</field>
      <field name="model">imprint.process</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="name"/>
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="output_format_tree">
      <field name="name">Output Format</field>
      <field name="model">output.format</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="name"/>
        </tree>
      </field>
    </record>
   <record model="ir.ui.view" id="turnaround_time_tree">
      <field name="name">Turnaround Time</field>
      <field name="model">turnaround.time</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="name"/>
        </tree>
      </field>
    </record>

    <!-- actions opening views on models -->
    
    <record model="ir.actions.act_window" id="promoartwork.orders_action_window">
      <field name="name">Virtual Requests</field>
      <field name="res_model">virtual.artwork</field>
      <field name="view_mode">tree,form</field>
    </record>
    
    <record model="ir.actions.act_window" id="promoartwork.request_quotes_action_window">
      <field name="name">Request Quotes</field>
      <field name="res_model">request.quote</field>
      <field name="view_mode">tree,form</field>
    </record>

    <record model="ir.actions.act_window" id="promoartwork.job_types_action_window">
      <field name="name">Job Types</field>
      <field name="res_model">job.types</field>
      <field name="view_mode">tree</field>
    </record>

    <record model="ir.actions.act_window" id="promoartwork.size_properties_action_window">
      <field name="name">Size Properties</field>
      <field name="res_model">size.properties</field>
      <field name="view_mode">tree</field>
    </record>

    <record model="ir.actions.act_window" id="promoartwork.imprint_colors_action_window">
      <field name="name">Colors</field>
      <field name="res_model">imprint.colors</field>
      <field name="view_mode">tree</field>
    </record>

    <record model="ir.actions.act_window" id="promoartwork.imprint_process_action_window">
      <field name="name">Imprint Process</field>
      <field name="res_model">imprint.process</field>
      <field name="view_mode">tree</field>
    </record>

    <record model="ir.actions.act_window" id="promoartwork.output_format_action_window">
      <field name="name">Output Format</field>
      <field name="res_model">output.format</field>
      <field name="view_mode">tree</field>
    </record>

    <record model="ir.actions.act_window" id="promoartwork.turnaround_time_action_window">
      <field name="name">Turnaround Time</field>
      <field name="res_model">turnaround.time</field>
      <field name="view_mode">tree</field>
    </record>
   

    <!-- Top menu item -->
    
    <menuitem name="Promoartwork" id="promoartwork.menu_root"/>
   
    <!-- menu categories -->
    
    <menuitem name="Orders" id="promoartwork.menu_1" parent="promoartwork.menu_root"/>
   
    <!-- actions -->
    
    <menuitem name="Artwork Orders" id="promoartwork.menu_1_list" parent="promoartwork.menu_1"
              action="promoartwork.orders_action_window"/>
    <menuitem name="Request Quotes" id="promoartwork.menu_2_list" parent="promoartwork.menu_1"
              action="promoartwork.request_quotes_action_window"/>
   
   <menuitem name="Master Data" id="promoartwork.master_data" parent="promoartwork.menu_root"/>
    <menuitem name="Job Types" id="promoartwork.job_types" parent="promoartwork.master_data"
              action="promoartwork.job_types_action_window"/>
    <menuitem name="Size Properties" id="promoartwork.size_properties" parent="promoartwork.master_data"
              action="promoartwork.size_properties_action_window"/>
    <menuitem name="Colors" id="promoartwork.imprint_colors" parent="promoartwork.master_data"
              action="promoartwork.imprint_colors_action_window"/>
    <menuitem name="Imprint Process" id="promoartwork.imprint_process" parent="promoartwork.master_data"
              action="promoartwork.imprint_process_action_window"/>
    <menuitem name="Output Format" id="promoartwork.output_format" parent="promoartwork.master_data"
              action="promoartwork.output_format_action_window"/>
    <menuitem name="Turnaround Time" id="promoartwork.turnaround_time" parent="promoartwork.master_data"
              action="promoartwork.turnaround_time_action_window"/>
  </data>
</odoo>