                                    <div class="col-md-3">Imprint Process</div>
                                    <div class="col-md-9">
                                        <select class="imprint_process form-control o_website_form_input" name="imprint_process" id="imprint_process" >
                                            <option selected="selected">--SELECT--</option>
                                            <t t-foreach="imprint_process" t-as="process">
                                                <option t-att-value="process.id"><t t-esc="process.name" /></option>
                                            </t>
                                        </select>
                                    </div>
                                </div>
                                <div class="row" style="padding-top:10px;">
                                    <div class="col-md-3">Output Format</div>
                                    <div class="col-md-9">
                                        <select class="output_format form-control o_website_form_input" name="output_format" id="output_format" >
                                            <option selected="selected">--SELECT--</option>
                                            <t t-foreach="output_format" t-as="format">
                                                <option t-att-value="format.id"><t t-esc="format.name" /></option>
                                            </t>
                                        </select>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12"><h3>Delivery</h3></div>
                                </div>
                                <div class="row" style="padding-top:10px;">
                                    <div class="col-md-3">Upload File (jpg, gif, zip or pdf)</div>
                                    <div class="col-md-9">
                                        <input type="file" class="" accept="image/*, .pdf, .zip" id="requested_file" name="requested_file"/>
                                    </div>
                                </div>
                                <div class="row" style="padding-top:10px;">
                                    <div class="col-md-3">Turnaround Time</div>
                                    <div class="col-md-9">
                                        <select class="imprint_process form-control o_website_form_input" name="turnaround_time" id="turnaround_time" >
                                            <option selected="selected">--SELECT--</option>
                                            <t t-foreach="turnaround_time" t-as="tot">
                                                <option t-att-value="tot.id"><t t-esc="tot.name" /></option>
                                            </t>
                                        </select>
                                    </div>
                                </div>
                                <div class="row" style="padding-top:10px;">
                                    <div class="col-md-3">Instructions / Comments</div>
                                    <div class="col-md-9">
                                        <textarea type="file" class="form-control" rows="4" id="instructions" name="instructions"/>
                                    </div>
                                </div>
                                <div class="row" style="padding-top:10px;">
                                    <div class="col-md-3">Expected Charges</div>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="expected_charges" name="expected_charges"/>
                                    </div>
                                </div>
                                <div class="row" style="padding-top:10px;">
                                    <div class="col-md-12 text-center">
                                        <input type="submit" class="btn btn-primary btn-lg o_website_form_send"/>
                                        <input type="reset" class="btn btn-primary btn-lg o_website_form_send"/>
                                        <span id="o_website_form_result"></span> 
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                    
                </div>
            </t>
        </template>
        
